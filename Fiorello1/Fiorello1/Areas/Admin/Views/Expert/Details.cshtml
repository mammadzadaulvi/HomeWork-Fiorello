@using Fiorello1.Areas.Admin.ViewModels.Expert
@model ExpertDetailsViewModel


<div class="col-lg-12 grid-margin ">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Product Details</h4>
        </div>
        <div class="table-responsive">
            <table class="table">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Fullname</th>
                        <th>Position</th>
                        <th>Photo</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>@Model.Id</td>
                        <td>@Model.Fullname</td>
                        <td>@Model.Position</td>
                        <td><img src="~/assets/img/@Model.PhotoPath" /></td>
                    </tr>
                </tbody>
            </table>
            <a asp-action="index" asp-controller="expert" class="btn btn-info">Cancel</a>
        </div>
    </div>
</div>