@using Fiorello1.Areas.Admin.ViewModels.FlowerExpert
@model FlowerExpertDetailsViewModel

<div class="col-lg-12 grid-margin ">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Flower Expert Details</h4>
        </div>
        <div class="table-responsive">
            <table class="table">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Title</th>
                        <th>Description</th>
                        <th>Expert Name</th>
                        <th>Expert Position</th>
                        <th>Photo</th>
                        <th>Actions</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>@Model.Id</td>
                        <td>@Model.Title</td>
                        <td>@Model.Description</td>
                        <td>@Model.ExpertName</td>
                        <td>@Model.ExpertPosition</td>
                        <td><img src="~/assets/img/@Model.PhotoPath" /></td>
                    </tr>
                </tbody>
            </table>
            <a asp-action="index" asp-controller="flowerexpert" class="btn btn-info">Cancel</a>
        </div>
    </div>
</div>
</div>